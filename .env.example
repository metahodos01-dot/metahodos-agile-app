# Anthropic API (Phase 8+ - AI Features)
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Application Settings
VITE_APP_NAME=Metahodos Agile App
VITE_APP_VERSION=0.1.0
